<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.insiteprojectid.practiceadapter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MenuUtama">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentActivity"
            android:label="@string/student_list_title"/>
        <activity android:name=".TeacherActivity" />
        <activity
            android:name=".AddStudentActivity"
            android:label="@string/add_activity_title"
            />
        <activity
            android:name=".EditStudentActivity"
            android:label="@string/title_activity_edit_student"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
